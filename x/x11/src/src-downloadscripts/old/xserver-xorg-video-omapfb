# OMAP driver
# http://packages.debian.org/sid/xserver-xorg-video-omapfb
cd /tmp
rm -rf xf86-video-omapfb
mkdir xf86-video-omapfb ; cd xf86-video-omapfb
wget http://ftp.de.debian.org/debian/pool/main/x/xf86-video-omapfb/xf86-video-omapfb_0.1.1.1.orig.tar.gz
gunzip xf86-video-omapfb_0.1.1.1.orig.tar.gz
xz -ze9 xf86-video-omapfb_0.1.1.1.orig.tar
mv -fv xf86-video-omapfb_0.1.1.1.orig.tar.xz xf86-video-omapfb_0.1.1.1.tar.xz
ls -1 $PWD/*z
